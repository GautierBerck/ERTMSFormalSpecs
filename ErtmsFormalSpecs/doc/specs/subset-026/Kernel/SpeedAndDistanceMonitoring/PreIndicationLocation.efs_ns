 Guid="92a5fe74-f56f-49fb-ad9b-59302ec14d54"
><Comment></Comment>
PreIndicationLocation.d_I ( PreIndicationLocation.TargetSpeed(MRSPnTarget) ) &lt;= PreIndicationLocation.MRSPnEnd(MRSPnTarget)</PreCondition><PreCondition
 Guid="78b9ae74-6271-4ba4-ba19-6547ce837dfe"
>MRSPnTarget == PreIndicationLocation.FirstTargetFromLocation(TrainPosition.FrontEndPosition(Default.DistanceInterval.Nom))</PreCondition></PreConditions>
<Expression>PreIndicationLocation.d_I ( PreIndicationLocation.TargetSpeed(MRSPnTarget) ) - BaseTypes.SpeedToMS(TargetSpeed(MRSPnTarget)) * PreIndicationLocation.T_preindication</Expression>
</Case><Case
 Name="Second case, first target"
 Guid="3189c7a3-2665-41cc-992e-f234cfbde7d0"
>
<Comment></Comment>
<PreConditions>
<PreCondition
 Guid="14678d4c-8a8c-49e3-99ba-0284c32a0363"
>MRSPnTarget == PreIndicationLocation.FirstTargetFromLocation(TrainPosition.FrontEndPosition(Default.DistanceInterval.Nom))</PreCondition><PreCondition
 Guid="fea3478d-454f-4a14-b807-f7c9564ad8a4"
>PreIndicationLocation.MRSPnEnd(MRSPnTarget) &lt; PreIndicationLocation.d_I ( PreIndicationLocation.TargetSpeed(MRSPnTarget) )</PreCondition><PreCondition
 Guid="75a229f0-84ec-4f4b-aa86-b24afaa035d4"
><Comment></Comment>
PreIndicationLocation.d_I( PreIndicationLocation.FirstTargetFromLocation(MRSPnTarget.Location).Speed) &lt;= PreIndicationLocation.MRSPnEnd(MRSPnTarget)</PreCondition></PreConditions>
<Expression>PreIndicationLocation.MRSPnEnd(MRSPnTarget)- BaseTypes.SpeedToMS(PreIndicationLocation.TargetSpeed(MRSPnTarget)) * PreIndicationLocation.T_preindication</Expression>
</Case><Case
 Name="EOA, first case"
 Guid="e4ed753d-ac09-403e-afd3-fb72a28b9706"
>
<PreConditions>
<PreCondition
 Guid="0b29228c-ccce-4669-acfd-3f9141ede40a"
>PreIndicationLocation.d_I ( PreIndicationLocation.TargetSpeed(MRSPnTarget) ) &lt;= PreIndicationLocation.MRSPnEnd(MRSPnTarget)</PreCondition><PreCondition
 Guid="a697db5b-8d80-4750-948c-b0b45689adc2"
>PreIndicationLocation.MRSPnStart(MRSPnTarget) &lt; PreIndicationLocation.d_I ( PreIndicationLocation.TargetSpeed(MRSPnTarget) )</PreCondition><PreCondition
 Guid="b45489d6-5cf3-4f02-83f3-ec715922f527"
>MA.IsEOA ( MRSPnTarget )</PreCondition></PreConditions>
<Expression>PreIndicationLocation.d_I_EOA ( PreIndicationLocation.TargetSpeed(MRSPnTarget) ) - BaseTypes.SpeedToMS(TargetSpeed(MRSPnTarget)) * PreIndicationLocation.T_preindication</Expression>
</Case><Case
 Name="Other target, first case"
 Guid="41f0a5a1-7965-4568-834c-248d28641324"
>
<Comment></Comment>
<PreConditions>
<PreCondition
 Guid="77374c64-30fb-4792-b9cd-84aa69ffd20f"
>PreIndicationLocation.d_I ( PreIndicationLocation.TargetSpeed(MRSPnTarget) ) &lt;= PreIndicationLocation.MRSPnEnd(MRSPnTarget)</PreCondition><PreCondition
 Guid="dcee462f-c5ab-4b6b-b370-8eb19ec9d5b9"
>PreIndicationLocation.MRSPnStart(MRSPnTarget) &lt; PreIndicationLocation.d_I ( PreIndicationLocation.TargetSpeed(MRSPnTarget) )</PreCondition></PreConditions>
<Expression>PreIndicationLocation.d_I ( PreIndicationLocation.TargetSpeed(MRSPnTarget) ) - BaseTypes.SpeedToMS(TargetSpeed(MRSPnTarget)) * PreIndicationLocation.T_preindication</Expression>
</Case><Case
 Name="Second case"
 Guid="fa19f559-65e4-402a-897b-7fda2bfa28dd"
>
<Comment></Comment>
<PreConditions>
<PreCondition
 Guid="e0a60a90-5e23-4565-820e-7eeb897d42f6"
><Comment></Comment>
PreIndicationLocation.d_I(PreIndicationLocation.FirstTargetFromLocation(MRSPnTarget.Location).Speed) &lt;= PreIndicationLocation.MRSPnEnd(MRSPnTarget)</PreCondition><PreCondition
 Guid="ca3a5b34-272b-4664-a001-2443e4f76a1d"
>PreIndicationLocation.MRSPnEnd(MRSPnTarget) &lt; PreIndicationLocation.d_I ( PreIndicationLocation.TargetSpeed(MRSPnTarget) )</PreCondition></PreConditions>
<Expression>MRSPnEnd(MRSPnTarget) - BaseTypes.SpeedToMS(TargetSpeed(MRSPnTarget)) * T_preindication</Expression>
</Case></Cases>
